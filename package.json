{
  "name": "Money",
  "version": "1.0.0",
  "description": "Mobile Web for Carousell",
  "main": "index.js",
  "author": "Trong Nhan <imouto1994@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev-client-build-watch": ". ./config.dev.sh && NODE_ENV=development node ./webpack",
    "dev-server-build-watch": ". ./config.dev.sh && NODE_ENV=development webpack --config ./webpack/server.dev.js",
    "dev-server": ". ./config.dev.sh && NODE_ENV=development nodemon public/build/server/main.js",
    "prod-client-build": ". ./config.sh && [ -d public/build/client ] && rm -r public/build/client; NODE_ENV=production webpack --config ./webpack/client.prod.js",
    "prod-server-build": ". ./config.sh && [ -d public/build/server ] && rm -r public/build/server; NODE_ENV=production webpack --config ./webpack/server.prod.js",
    "prod-server": ". ./config.sh && NODE_ENV=production node public/build/server/main.js",
    "prod-client-stats": ". ./config.sh && [ -d public/build/client ] && rm -r public/build/client; NODE_ENV=production webpack --config ./webpack/client.prod.js --profile --json > stats.json",
    "lint": "eslint src config gulp tests webpack index.js feature.js postcss.config.js",
    "format": "prettier --trailing-comma es5 --write '{src,tests,webpack,config,gulp}/**/*.js'",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --trailing-comma es5",
      "git add"
    ]
  },
